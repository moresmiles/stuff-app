<div class="section login">
  <div class="container">
      <div class="one-half column">
        <h1 class="section-heading"> New Box </h1>

        <form action="/moves/<%=@move.id%>/boxes" method="post">
          <label> Name </label>
            <input type="text" name="name" placeholder="Box Name" id="name">
          <label> Location </label>
            <input type="text" name="location" placeholder="New Apt, Storage, Parents Basement, etc..."id="location">
            <input type="submit" id="submit" value="Create new Box">
        </form>

          <div class="row">
            <div class="one-third column offset-by-one">
              <form action="/moves/<%= @move.id %>/boxes">
                <input class="button button-primary" type="submit" value="Back">
              </form>
            </div>
          </div>

    </div>
  </div>
</div>
